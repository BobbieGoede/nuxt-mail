  "funding": "https://github.com/sponsors/bobbiegoede",
  "license": "MIT",
  "author": "Bobbie Goede",
  "type": "module",
  "files": [
    "dist"
  ],
  "resolutions": {
    "@goede/nuxt-mail": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
